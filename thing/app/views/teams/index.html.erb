<h1>TEAMS!</h1>


<%= link_to 'New Team', new_team_path %>
<table>
  <tr>
   <th>Team</th>
   <th colspan="3"></th>
  </tr>
  
  <% @teams.each do |team| %>
    <tr>
      <td><%= team.name %></td>
      <td><%= team.text %></td>
      <td><%= link_to "Edit", edit_team_path(team) %></td>
   </tr>
  <% end %>
</table>



